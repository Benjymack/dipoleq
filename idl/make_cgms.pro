; effi_plotpro make_cgms,fname0,lab=lab,ps=pscommon makecgms_common,fname     if keyword_set(fname0) then begin     	    fname = fname0     	endif else begin	    if not keyword_set(fname) then fname = dialog_pickfile(/read) 	endelse		get_hdf_data,fname,"Psi",Psi,x,z,$	labels=labels,units=units,range=psi_range		psilev = findgen(15)*(psi_range[1]-psi_range[0])/14.+psi_range[0]				get_hdf_data,fname,"Mod B2",B2,$	labels=B2_l		b = sqrt(b2[1:*,*])*28		labs = intarr(5)*0		if keyword_set(lab) then begin	   labs = labs + 1	   print,"Blimey!"	endif	   	freqs = [2.45,6,9,18,28]			if keyword_set(ps) then begin	    oldp = !d.name	    set_plot,"ps"	    device,encap=0	    device,file=string(ps)+".ps"	    oldf = !p.font	    !p.font=0	endif	asp = 1.000000	contour,psi,x,z,xsty=1,ysty=1,levels=psilev,xrange=[0,2.5],yrange=[-1, 1.5],$	    xtitle="R (m)", ytitle="Z (m)",position = ASPECT(asp,margin=.08)	contour,/over,b,x[1:*],z,levels=freqs,c_lines=2,/follow,c_labels=labs	contour,/over,b,x[1:*],z,levels=freqs/2,c_lines=4,/follow,c_labels=intarr(5)*0     if keyword_set(ps) then begin	    device,/close	    set_plot,oldp	    !p.font=oldf	endifend