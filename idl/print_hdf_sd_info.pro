pro print_hdf_sd_info,fname		if not keyword_set(fname) then fname = dialog_pickfile(/read,filter="*.hdf")	if (not hdf_ishdf(fname)) then begin		print,"Bad HDF file... quitting."		return	endif		sd_id = HDF_SD_START(fname,/READ)	hdf_sd_fileinfo, sd_id, nsds, natt		for i=0, nsds -1 do begin		sds_id = hdf_sd_select(sd_id,i)		if not hdf_sd_iscoordvar(sds_id) then begin			HDF_SD_GETINFO, SDS_ID , CALDATA=caldata , COORDSYS=coordsys , DIMS=dims , $			FILL=fill, FORMAT=format , HDF_TYPE=hdf_type, LABEL=label, NAME=name, $			NATTS=natts, NDIMS=ndims, RANGE=range, TYPE=type, UNIT=unit			if not keyword_set(range) then range = "NOT SET"			if not keyword_set(unit) then unit = "NOT SET"			if not keyword_set(caldata) then caldata = "NOT SET"			if not keyword_set(format) then format = "NOT SET"			if not keyword_set(fill) then fill = "NOT SET"			if not keyword_set(coordsys) then coordsys = "NOT SET"			print,'Name = "',name,'", Label = "',label,'", Unit = "',unit,'", Type = "',type,'"'			print,"       dims = ",ndims,dims,", attr = ",natts,", Range = ",range		endif		hdf_sd_endaccess,sds_id	endfor		hdf_sd_end,sd_idend